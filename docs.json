{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "willow",
  "name": "EditWage Documentation",
  "colors": {
    "primary": "#0070cc",
    "light": "#0070cc",
    "dark": "#0070cc"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "icon": "folder-bookmark",
        "languages": [
          {
            "language": "fr",
            "default": true,
            "groups": [
              {
                "group": "Débuter",
                "pages": [
                  "documentation/fr/getting-started/history",
                  "index"
                ]
              }
            ]
          }
        ]
      },
      {
        "tab": "Nouveautés",
        "icon": "bolt",
        "pages": [
          "new"
        ]
      }
    ]
  },
  "logo": "/images/EDITWAGE-SQUARE3.png",
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:contact@editwage.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/editwage/",
      "discord": "https://discord.gg/mHNCvwpgct",
      "website": "https://editwage.com/"
    }
  },
  "appearance": {
    "strict": false,
    "default": "dark"
  },
  "fonts": {
    "family": "Inter"
  },
  "integrations": {
    "ga4": {
      "measurementId": "G-SFE8XNXG6Q"
    }
  },
  "metadata": {
    "timestamp": false
  },
  "favicon": "/images/EDITWAGE-SQUARE3.png",
  "seo": {
    "indexing": "all"
  },
  "errors": {
    "404": {
      "title": "Oops... We're lost (404)"
    }
  },
  "banner": {
    "dismissible": true,
    "content": "Besoin d'aide ? Nous y sommes !"
  },
  "background": {
    "color": {
      "dark": "#222A38"
    }
  },
  "description": "Besoin d'aide ? Nous y sommes !",
  "search": {
    "prompt": "Chercher ou demander..."
  },
  "interaction": {
    "drilldown": false
  }
}
